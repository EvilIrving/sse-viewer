{
  "name": "SSE Viewer",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Capture and visualize Server-Sent Events (SSE) in DevTools",
  "devtools_page": "devtools.html",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "background": { 
    "service_worker": "background.js",
    "type": "module"
  }
}